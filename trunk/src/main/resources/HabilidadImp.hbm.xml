<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<typedef name="Posicion" class="org.hibernate.type.EnumType">
		<param name="enumClass">Clases.Posicion</param>
	</typedef>

	<class name="Clases.HabilidadImp" table="Habilidad" catalog="public">
		<id name="id" type="java.lang.Integer">
			<column name="H_ID" />
			<generator class="sequence" />
		</id>

		<property name="posicion" type="Posicion">
			<column name="H_POSICION" not-null="true" unique="false" />
		</property>


	</class>
</hibernate-mapping>