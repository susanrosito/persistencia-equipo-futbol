<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dominio.FormacionStrategyImp" table="FormacionStrategyImp" catalog="public">
		<id name="id" type="java.lang.Integer">
			<column name="FSI_ID" />
			<generator class="sequence" />
		</id>
		
		<one-to-one name="equipo" cascade="all"></one-to-one>

		<bag name="titulares" table="Titulares" cascade="all,delete-orphan">
			<key column="formacion_id" />
			<one-to-many class="dominio.Titular" />
		</bag>
		
		<bag name="posiciones" table="Posiciones" cascade="all">
			<key column="formacionStImp_id" />
			<column name="H_POSICION" not-null="true" unique="false" />
		</bag>
	</class>
</hibernate-mapping>
