<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Clases.FormacionStrategyImp" table="Formaciones" catalog="public">
		<id name="id" type="java.lang.Integer">
			<column name="F_ID" />
			<generator class="sequence" />
		</id>
		
		<many-to-one name="equipo" cascade="all"></many-to-one>

		<bag name="posiciones" table="Posiciones" cascade="all,delete-orphan">
			<key column="F_id" />
			<one-to-many class="Clases.Posicion" />
		</bag>
	</class>
</hibernate-mapping>