<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="Clases.PartidoDeCopa" table="PartidoDeCopa" catalog="public">
        <id name="id" type="java.lang.Integer">
            <column name="PC_ID" />
            <generator class="sequence" />
        </id>
	<many-to-one name="primerPartido" cascade="all" ></many-to-one>
	<many-to-one name="segundoPartido" cascade="all" ></many-to-one>
	<many-to-one name="equipoA" cascade="all" ></many-to-one>
	<many-to-one name="equipoB" cascade="all" ></many-to-one>
	<property name="penalesConvertidosEquipoA" type="int">
	            <column name="PC_PENALESCONVEQUIPOA" not-null="true" unique="false" />
        </property>
	<property name="penalesConvertidosEquipoA" type="int">
	            <column name="PC_PENALESCONVEQUIPOB" not-null="true" unique="false" />
        </property>
    </class>
</hibernate-mapping>
